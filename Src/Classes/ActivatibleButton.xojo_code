#tag Class
Protected Class ActivatibleButton
Inherits WebLabel
	#tag Event
		Sub Pressed()
		  if me.isActive then
		    RaiseEvent PressedButton
		    
		  end if
		End Sub
	#tag EndEvent


	#tag Method, Flags = &h0
		Sub activate()
		  isActive = true
		  'me.Style = ButtonStyle_Primary
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h0
		Sub deactivate()
		  isActive = false
		  'me.Style = ButtonStyle_Inactive
		End Sub
	#tag EndMethod


	#tag Hook, Flags = &h0
		Event PressedButton()
	#tag EndHook


	#tag Property, Flags = &h21
		Private isActive As boolean
	#tag EndProperty


