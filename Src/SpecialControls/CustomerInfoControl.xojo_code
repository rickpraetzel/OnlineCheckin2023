	#tag EndMethod

	#tag Method, Flags = &h21
		Private Function getCustomer() As bookingtoolkit.customer
		  mCustomer.name = new BookingToolkit.DisplayName(FirstnameField.Text, LastnameField.Text)
		  mCustomer.zip = ZipField.Text
		  if instr(PhoneField.Text, "X") = 0 then 
		    mCustomer.phoneNumber = new Bookingtoolkit.PhoneNumber(PhoneField.Text)
		    
		  end if 
		  //email is handled by the validator
		  
		  return mCustomer
		End Function
	#tag EndMethod

	#tag Method, Flags = &h21
		Private Function getMaskedEmail(email as String) As string
		  if email = "" then
		    return ""
		    
		  end if
		  
		  dim suffix as string
		  if instr(email, "@") > 0 then
		    suffix = Right(email, len(email) - instr(email, "@") + 1)
		    
		  end if
		  
		  return left(email, 4) + "******" + suffix
		End Function
	#tag EndMethod

