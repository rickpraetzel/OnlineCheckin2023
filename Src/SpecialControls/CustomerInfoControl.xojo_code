#tag Events ZipField
	#tag Event
		Sub TextChanged()
		  if len(me.Text) >= 5 then
		    dim req as new USPS.GetCityStateFromZip(me.Text)
		    controller1.processRequest(req) 
		    
		  end if
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events Controller1
	#tag Event
		Sub Error(error as USPS.ErrorResponse)
		  app.logEntry("USPS error: " + error.reason)
		End Sub
	#tag EndEvent
	#tag Event
		Sub NetworkError(err as RuntimeException)
		  app.logEntry(537281, "USPS network error", err)
		End Sub
	#tag EndEvent
	#tag Event
		Sub ResponseReceived(response as USPS.Parser)
		  mCustomer.city = USPS.CityAndStateResponse(response).city
		  mCustomer.state = USPS.CityAndStateResponse(response).state
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events Validator
	#tag Event
		Sub Error(msg as string)
		  RaiseEvent Error("Email validation error: " + msg)
		End Sub
	#tag EndEvent
	#tag Event
		Sub ValidationComplete(response as MailGun.ValidationResponse)
		  isValidEmailAddress = response.isValidAddress
		  EventTimer.runMode = webtimer.runmodes.Single
		  EventTimer.Reset()
		  
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events EventTimer
	#tag Event
		Sub Run()
		  if isValidEmailAddress then
		    session.logEntry("Event timer firing. Email found to be valid")
		    if InStr(EmailField.Text, "*") = 0 then 
		      mCustomer.email = EmailField.Text
		      
		    end if
		    
		    if not allTextFieldsValid() then
		      RaiseEvent Invalid
		      
		      return 
		      
		    end if 
		    
		    RaiseEvent ValidCustomerReady(getCustomer())
		    
		  else
		    session.logEntry("Event timer firing. Email found to be Invalid")
		    
		    RaiseEvent invalid
		    
		  end if
		End Sub
	#tag EndEvent
#tag EndEvents
