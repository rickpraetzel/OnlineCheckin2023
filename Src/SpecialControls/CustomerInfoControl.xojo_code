	#tag Method, Flags = &h0
		Sub showCustomer(cust as bookingtoolkit.customer)
		  FirstnameField.Text = cust.name.firstname
		  LastnameField.Text = cust.name.lastname
		  ZipField.Text = cust.zip
		  if isMaskingContent then
		    PhoneField.Text = cust.phoneNumber.getMaskedNumber()
		    EmailField.Text = getMaskedEmail(cust.email)
		    
		  else
		    PhoneField.Text = cust.phoneNumber.getFormattedNumber()
		    EmailField.Text = cust.email
		    
		  end if
		  
		  isValidEmailAddress = cust.email <> "" //Assume that if we have an email it is valid
		  mCustomer = cust
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h0
		Sub validate()
		  dim emailNeedsValidation as boolean =_
		  (not isValidEmailAddress) or (mCustomer.isInDatabase and instr(EmailField.Text, "*") = 0)
		  
		  if emailNeedsValidation then
		    session.logEntry("Calling external API for email validation")
		    Validator.validateEmailAddress(EmailField.Text)
		    
		    return 
		    
		  end if
		  
		  EventTimer.runMode = WebTimer.runmodes.single
		  
		  EventTimer.Reset()
		End Sub
	#tag EndMethod


