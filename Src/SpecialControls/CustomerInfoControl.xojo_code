#tag WindowCode
	#tag Event
		Sub Opening()
		  dim cred as new USPS.Credentials()
		  cred.userID = stringToText(app.preferences.Lookup("uspsID", ""))
		  cred.password = stringToText(app.preferences.Lookup("uspsPW", ""))
		  
		  Controller1.setCredentials(cred)
		  
		  dim auth as new MailGun.Authentication(_
		  app.preferences.Lookup("emailValidationEndpoint", ""),_
		  app.preferences.lookup("emailKey", ""))
		  
		  validator.setCredentials(auth)
		End Sub
	#tag EndEvent


	#tag Method, Flags = &h21
		Private Function allTextFieldsValid() As boolean
		  if FirstnameField.Text = "" then
		    return false
		    
		  end if
		  
		  if LastnameField.Text = "" then
		    return false
		    
		  end if
		  
		  if PhoneField.Text = "" then
		    return false
		    
		  end if
		  
		  if ZipField.Text = "" then
		    return false
		    
		  end if
		  
		  return true
		End Function
	#tag EndMethod

	#tag Method, Flags = &h0
		Sub clearCustomer()
		  FirstnameField.Text = ""
		  LastnameField.Text = ""
		  EmailField.Text = ""
		  PhoneField.Text = ""
		  ZipField.Text = ""
		  mCustomer = new bookingtoolkit.customer()
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h0
